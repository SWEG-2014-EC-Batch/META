#include <iostream>
#include <cmath>
using namespace std;


double hypotenuse(double side1, double side2) {

  return sqrt(pow(side1,2) + pow(side2,2));
}


int main() {
  
  double side1, side2;
  cout << "Enter the length of side1: ";
  cin >> side1;
  cout << "Enter the length of side2: ";
  cin >> side2;

 
  double result = hypotenuse(side1, side2);


  cout << "The hypotenuse of the right triangle is: " << result << endl;

  return 0;
}
